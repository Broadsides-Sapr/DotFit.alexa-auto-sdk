    <string name="ttt_getting_started_text1">\"Alexa, आस-पास का कोई पेट्रॉल पंप ढूंढो.\"</string>
    <string name="ttt_getting_started_text2">\"Alexa, म्यूज़िक चलाओ.\"</string>
    <string name="ttt_getting_started_text3">\"Alexa, मौसम कैसा है?\"</string>
    <string name="ttt_getting_started_text4">\"Alexa, रास्ते पर ट्रैफ़िक कैसा है?\"</string>
    <string name="ttt_getting_started_text5">\"Alexa, मेरे हाइलाइट क्या हैं?\"</string>
    <string name="ttt_alexa_s_talents_text1">\"Alexa, गुड मॉर्निंग.\"</string>
    <string name="ttt_alexa_s_talents_text2">\"Alexa, कहानी सुनाओ.\"</string>
    <string name="ttt_alexa_s_talents_text3">\"Alexa, क्या-क्या चीज़ें याद रखी जा सकती है?\"</string>
    <string name="ttt_entertainment_text1">\"Alexa, म्यूज़िक चलाओ.\"</string>
    <string name="ttt_entertainment_text2">\"Alexa, 70 के दशक का रॉक म्यूज़िक चलाओ.\"</string>
    <string name="ttt_entertainment_text3">\"Alexa, Bruno Mars की नई एल्बम चलाओ.\"</string>
    <string name="ttt_entertainment_text4">\"Alexa, मेरी Audible बुक फिर से चलाओ.\"</string>
    <string name="ttt_communication_text1">\"Alexa,  रोहित को एक मैसेज भेजो.\"</string>
    <string name="ttt_communication_text2">\"Alexa, मैं मैसेज कैसे भेजूं?\"</string>
    <string name="ttt_communication_text3">\"Alexa,  राहुल के मोबाइल पर कॉल करो.\"</string>
    <string name="ttt_communication_text4">\"Alexa, 755–5747–330 पर कॉल करो.\"</string>
    <string name="ttt_weather_text1">\"Alexa, मौसम कैसा है?\"</string>
    <string name="ttt_weather_text2">\"Alexa, क्या आज बारिश होगी?\"</string>
    <string name="ttt_weather_text3">\"Alexa,  दिल्ली में कैसा मौसम है?\"</string>
    <string name="ttt_smart_home_text1">\"Alexa, बरामदे की लाइट चालू करो.\"</string>
    <string name="ttt_smart_home_text2">\"Alexa, सामने का दरवाज़ा बंद करो.\"</string>
    <string name="ttt_smart_home_text3">\"Alexa, घर का तापमान 21 डिग्री सेल्सियस पर सेट कर दो.\"</string>
    <string name="ttt_smart_home_text4">\"Alexa, गैराज का दरवाज़ा बंद करो.\"</string>
    <string name="ttt_news_and_information_text1">\"Alexa, समाचार बताओ?\"</string>
    <string name="ttt_news_and_information_text2">\"Alexa, मेरे खेलों के अपडेट क्या हैं?\"</string>
    <string name="ttt_news_and_information_text3">\"Alexa, DD News चलाओ.\"</string>
    <string name="ttt_news_and_information_text4">\"Alexa, NDTV चलाओ.\"</string>
    <string name="ttt_navigation_text1">\"Alexa, आस-पास टॉप रेटिंग वाला थाई रेस्टोरेंट ढूंढो.\"</string>
    <string name="ttt_navigation_text2">\"Alexa, मुझे एयरपोर्ट का रास्ता बताओ.\"</string>
    <string name="ttt_navigation_text3">\"Alexa, मुझे रेलवे स्टेशन का रास्ता बताओ.\"</string>
    <string name="ttt_traffic_information_text1">\"Alexa, ऑफ़िस जाने के रास्ते में ट्रैफ़िक कैसा है?\"</string>
    <string name="ttt_traffic_information_text2">\"Alexa, एयरपोर्ट के रास्ते में ट्रैफ़िक कैसा है?\"</string>
    <string name="ttt_skills_text1">\"Alexa, आपमें नई स्किल क्या हैं?\"</string>
    <string name="ttt_lists_text1">\"Alexa, आज मेरी टू-डू लिस्ट में क्या-क्या है?\"</string>
    <string name="ttt_lists_text2">\"Alexa, मेरी शॉपिंग लिस्ट में दूध ऐड करो\"</string>
    <string name="ttt_lists_text3">\"Alexa, आज मेरी शॉपिंग लिस्ट में क्या-क्या है?\"</string>
    <string name="ttt_lists_text4">\"Alexa, पैकिंग लिस्ट बनाओ.\"</string>
    <string name="ttt_shopping_text1">\"Alexa, मेरा सामान कहां है?\"</string>
    <string name="ttt_shopping_text2">\"Alexa, मेरी कार के लिए एयर फ़्रेशनर खरीदो.\"</string>
    <string name="ttt_shopping_text3">\"Alexa, टूथपेस्ट को फिर से ऑर्डर करो.\"</string>
    <string name="ttt_questions_and_answers_text1">\"Alexa, टोक्यो में अभी क्या समय हुआ है?\"</string>
    <string name="ttt_questions_and_answers_text2">\"Alexa, चांद कितनी दूर है?\"</string>
    <string name="ttt_questions_and_answers_text3">\"Alexa,  वर्ल्ड कप कब है?\"</string>
    <string name="ttt_sports_text1">\"Alexa, मेरे खेलों के अपडेट क्या हैं?\"</string>
    <string name="ttt_sports_text2">\"Alexa, IPL का फ़ाइनल कब है?\"</string>
    <string name="ttt_sports_text3">\"Alexa, मुंबई इंडियंस का अगला मैच कब है?\"</string>
    <string name="ttt_sports_text4">\"Alexa, क्रिकेट वर्ल्ड कप कब है?\"</string>
    <string name="ttt_sports_text5">\"Alexa, Wimbledon कब है?\"</string>
    <string name="ttt_sports_text6">\"Alexa, हॉकी इंडिया लीग कब है?\"</string>
    <string name="ttt_sports_text7">\"Alexa, पिछले मैच में कोलकाता नाईट राइडर्स का स्कोर क्या था?\"</string>
    <string name="ttt_sports_text8">\"Alexa, चेन्नई सुपर किंग्स ने कितने IPL जीते हैं?\"</string>
    <string name="ttt_calendar_text1">\"Alexa, मेरा दिन कैसा लगता है?\"</string>
    <string name="ttt_calendar_text2">\"Alexa, मेरा अगला अपॉइंटमेंट क्या है?\"</string>
    <string name="ttt_calendar_text3">\"Alexa, मेरे कैलेंडर में दोपहर 2:00 बजे कॉफ़ी चैट ऐड करो.\"</string>
